<resources>

    <style name="AppTheme" parent="android:Theme.Light.NoTitleBar.Fullscreen" />
	<style name="wrap_content">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	</style>
	
	<style name="table_cell" parent="wrap_content">
	    <item name="android:layout_margin">20dip</item>
	    <item name="android:layout_width">120dip</item>
	    <item name="android:layout_height">120dip</item>
	</style>
	
	<style name="table_cell_image_button" parent="wrap_content">
	    <item name="android:layout_margin">10dip</item>
	    <item name="android:onClick">onClick</item>
	    <item name="android:src">@drawable/icon_selector</item>
	    <item name="android:background">@android:color/transparent</item>
	</style>
	
	<style name="table_cell_image_text_view" parent="wrap_content">
	</style>
</resources>